<AnchorPane id="pane" prefHeight="370.0" prefWidth="500.0" stylesheets="@../image/background.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.addInformation.PickUpCarController">
   <children>
      <Label layoutX="110.0" layoutY="32.0" text="PARKING" textFill="#323130">
         <font>
            <Font name="SansSerif Bold" size="61.0" />
         </font>
      </Label>
      <Button fx:id="PayButton" layoutX="220.0" layoutY="329.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="60.0" style="-fx-background-color: #DAA520;" text="Pay">
         <font>
            <Font name="SansSerif Regular" size="18.0" />
         </font>
      </Button>
      <TextField fx:id="CarIdField" layoutX="207.0" layoutY="280.0" prefHeight="50.0" prefWidth="86.0" promptText="Car id" style="-fx-background-color: transparent; -fx-txt-color: #323130;">
         <font>
            <Font size="23.0" />
         </font>
      </TextField>
      <Label layoutX="58.0" layoutY="104.0" text="Our parking uses credit card system. &#10;Please, input your car id number and fill &#10;all fields in the next step, &#10;after clicking &quot;pay&quot; button.&#10;&#10;All rights are reserved.&#10;Best regards, &quot;Parking&quot; administrator" textFill="#323130">
         <font>
            <Font name="SansSerif Bold" size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
